{
    "github": {
        "silent": true
    },
     "routes": [
    {
      "src": "/next/(.*)",
      "dest": "https://chat-gpt-next-web-three-orcin-53.vercel.app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "https://chatgpt-web-six-mu.vercel.app/$1"
    }
  ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Real-IP",
                    "value": "$remote_addr"
                },
                {
                    "key": " X-Forwarded-For",
                    "value": "$proxy_add_x_forwarded_for"
                },
                {
                    "key": "Host",
                    "value": "$http_host"
                }
            ]
        }
    ]
}
